{
  "name": "variable-text-mapper",
  "version": "1.0.0",
  "description": "A plugin for mapping and transforming variable text",
  "main": "dist/code.js",
  "scripts": {
    "build": "cp src/code.js dist/code.js && node build-ui.js",
    "watch": "nodemon --watch src --ext js,html --exec 'npm run build'",
    "dev": "npm run build && npm run watch"
  },
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "node-fetch": "^2.6.7"
  },
  "keywords": [
    "figma",
    "plugin",
    "variable",
    "text",
    "mapper"
  ],
  "author": "",
  "license": "MIT"
}